$(document).ready(function(){$('.social-share-single-facebook').on('click',function(){var url=encodeURIComponent(share_url);var share_type='fb_share_count';url='https://www.facebook.com/sharer/sharer.php?u='+url;var name="sharewindow";var iWidth=710;var iHeight=370;var iTop=(window.screen.availHeight-30-iHeight)/2;var iLeft=(window.screen.availWidth-10-iWidth)/2;window.open(url,name,'height='+iHeight+',,innerHeight='+iHeight+',width='+iWidth+',innerWidth='+iWidth+',top='+iTop+',left='+iLeft+',toolbar=no,menubar=no,scrollbars=auto,resizeable=no,location=no,status=no')});$('.social-share-single-googleplus').on('click',function(){var url=encodeURIComponent(share_url);var share_type='gp_share_count';url='https://plus.google.com/share?url='+url;var name="sharewindow";var iWidth=710;var iHeight=370;var iTop=(window.screen.availHeight-30-iHeight)/2;var iLeft=(window.screen.availWidth-10-iWidth)/2;window.open(url,name,'height='+iHeight+',,innerHeight='+iHeight+',width='+iWidth+',innerWidth='+iWidth+',top='+iTop+',left='+iLeft+',toolbar=no,menubar=no,scrollbars=auto,resizeable=no,location=no,status=no')});});